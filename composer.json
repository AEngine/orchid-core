{
    "name": "aengine/orchid",
    "description": "PHP7 Lightweight Framework",
    "keywords": ["php", "framework", "AEngine", "Orchid"],
    "homepage": "https://gitlab.com/AEngine/Orchid/Core",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksey Ilyin",
            "email": "alksily@icloud.com",
            "homepage": "https://www.facebook.com/aleksey.ily",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "pimple/pimple": "^3.0",
        "psr/http-message": "~1.0"
    },
    "provide": {
        "psr/http-message-implementation": "1.0",
        "psr/container-implementation": "1.0"
    },
    "suggest": {
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-apache": "*"
    },
    "autoload": {
        "psr-4": {
            "AEngine\\Orchid\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-0": { "AEngine\\Orchid\\Functional\\": "tests/" }
    }
}
